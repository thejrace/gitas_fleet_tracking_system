<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="settings-tab-content" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.settings.FleetSettingsController">
   <children>
      <Label styleClass="lbl-gray1" text="Filodan veri alınan adresleri buradan düzenleyebilirsiniz." />
      <Label layoutX="10.0" layoutY="10.0" text="URL Ayarları">
         <styleClass>
            <String fx:value="lbl-yellow" />
            <String fx:value="lbl-h4" />
            <String fx:value="lbl-bold" />
         </styleClass>
      </Label>
      <Label layoutX="20.0" layoutY="20.0" styleClass="lbl-gray1" text="\%%VAR%% adresteki değişken kısmı temsil etmektedir." />
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label styleClass="lbl-gray1" text="Plan" />
            <TextField fx:id="uiPlanUrlInput" prefHeight="25.0" prefWidth="550.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="84.0">
         <children>
            <Label styleClass="lbl-gray1" text="PDKS" />
            <TextField fx:id="uiPDKSInput" prefHeight="25.0" prefWidth="550.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="124.0">
         <children>
            <Label styleClass="lbl-gray1" text="Hız" />
            <TextField fx:id="uiSpeedUrlInput" prefHeight="25.0" prefWidth="550.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label styleClass="lbl-gray1" text="Mesaj Giden" />
            <TextField fx:id="uiMessageOutInput" prefHeight="25.0" prefWidth="550.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="164.0">
         <children>
            <Label styleClass="lbl-gray1" text="Mesaj Gelen" />
            <TextField fx:id="uiMessageInInput" prefHeight="25.0" prefWidth="550.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="204.0">
         <children>
            <Label styleClass="lbl-gray1" text="Sürücü Detay" />
            <TextField fx:id="uiDriverDetailsUrlInput" prefHeight="25.0" prefWidth="550.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <Label layoutX="10.0" layoutY="27.0" text="Paralel İndirme Limit Ayarları">
         <styleClass>
            <String fx:value="lbl-yellow" />
            <String fx:value="lbl-h4" />
            <String fx:value="lbl-bold" />
         </styleClass>
      </Label>
      <Label layoutX="20.0" layoutY="52.0" styleClass="lbl-gray1" text="Paralel İndirme Limiti; filoya aynı anda yapılabilecek azami istek sayısını temsil etmektedir." />
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="284.0">
         <children>
            <Label styleClass="lbl-gray1" text="Limit" />
            <TextField fx:id="uiDownloadLimitInput" prefHeight="25.0" prefWidth="90.0" />
         </children>
         <styleClass>
            <String fx:value="input-control" />
            <String fx:value="align-label" />
         </styleClass>
      </HBox>
      <Button fx:id="uiSaveBtn" mnemonicParsing="false" text="Kaydet">
         <styleClass>
            <String fx:value="content-btn" />
            <String fx:value="content-btn-warning" />
         </styleClass>
      </Button>
   </children>
</VBox>
